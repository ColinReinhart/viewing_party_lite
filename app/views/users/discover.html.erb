<h1>Discover Movies</h1>
<%= link_to "Top Rated Movies", "/users/#{@user.id}/movies?q=top%20rated" %>

<%= form_with url: "/users/#{@user.id}/discover", method: :get, local: true do |f| %>
<%= f.label "Search By Movie Title" %>
<%= f.text_field  "Search By Movie Title"%>
<%= f.submit "Search" %>
<% end %>
